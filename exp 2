#include <stdio.h>
#include <stdlib.h>

int cmpfunc(const void *a, const void *b) {
    return (*(int*)a - *(int*)b);
}

int main() {
    int n, m;
    scanf("%d", &n);   
    
    int g[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &g[i]);
    
    scanf("%d", &m);   
    
    int s[m];
    for (int i = 0; i < m; i++)
        scanf("%d", &s[i]);

    qsort(g, n, sizeof(int), cmpfunc);
    qsort(s, m, sizeof(int), cmpfunc);

    int i = 0, j = 0, content = 0;

    while (i < n && j < m) {
        if (s[j] >= g[i]) {  
            content++;  
            i++;        
        }
        j++;  
    }

    printf("%d\n", content);
    return 0;
}
